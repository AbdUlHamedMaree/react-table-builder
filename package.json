{
  "name": "@mrii/react-table-builder",
  "version": "0.0.2",
  "private": false,
  "sideEffects": false,
  "description": "library to easily build tables using MUI grid",
  "repository": {
    "type": "git",
    "url": "https://github.com/AbdUlHamedMaree/react-table-builder.git"
  },
  "bugs": {
    "url": "https://github.com/AbdUlHamedMaree/react-table-builder/issues"
  },
  "homepage": "https://github.com/AbdUlHamedMaree/react-table-builder.git",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "author": {
    "email": "hmidmrii@gmail.com",
    "name": "Abd UlHameed Maree",
    "url": "https://www.linkedin.com/in/abd-ul-hameed-maree"
  },
  "source": "./src/index.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "react-component",
    "mui",
    "table",
    "grid",
    "mui-table",
    "mui-grid",
    "material-ui",
    "material design"
  ],
  "scripts": {
    "build": "rimraf dist && concurrently --kill-others-on-fail -n rollup,tsc -c magenta,cyan \"rollup -c\" \"yarn tsc\"",
    "build:watch": "concurrently -k -n rollup,tsc -c magenta,cyan \"rollup -c -w\" \"yarn tsc -w\"",
    "example:next:dev": "concurrently -k -n build,dev -c magenta,cyan \"yarn build:watch -p none\" \"yarn --cwd nextjs-example dev\"",
    "example:react:dev": "concurrently -k -n build,dev -c magenta,cyan \"yarn build:watch -p none\" \"yarn --cwd reactjs-example start\"",
    "test": "jest",
    "lint": "yarn type-check && yarn eslint --quiet",
    "eslint:fix": "yarn eslint --fix",
    "type-check": "tsc -p ./tsconfig.eslint.json",
    "tsc": "tsc",
    "eslint": "eslint .",
    "prettier": "prettier --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\"",
    "prepack": "yarn build"
  },
  "devDependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.6",
    "@mui/lab": "^5.0.0-alpha.102",
    "@mui/material": "^5.10.8",
    "@mui/x-data-grid": "^5.17.6",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^14.1.0",
    "@swc/core": "^1.3.6",
    "@swc/jest": "^0.2.23",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.1.2",
    "@types/lodash.get": "^4.4.7",
    "@types/node": "^18.8.3",
    "@types/numeral": "^2.0.2",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "concurrently": "^7.4.0",
    "date-fns": "^2.29.3",
    "eslint": "^8.25.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "immer": "^9.0.15",
    "jest": "^29.1.2",
    "lodash.get": "^4.4.2",
    "numeral": "^2.0.6",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-swc3": "^0.6.0",
    "rollup-plugin-typescript-paths": "^1.4.0",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4",
    "zustand": "^4.1.1"
  },
  "peerDependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.6",
    "@mui/lab": "^5.0.0-alpha.102",
    "@mui/material": "^5.10.8",
    "@mui/x-data-grid": "^5.17.6",
    "date-fns": "^2.29.3",
    "immer": "^9.0.15",
    "lodash.get": "^4.4.2",
    "numeral": "^2.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zustand": "^4.1.1"
  }
}
